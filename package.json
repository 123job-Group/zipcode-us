{
  "name": "zipcode-us",
  "version": "1.0.0",
  "description": "Lambda function for zipcode-us",
  "scripts": {
    "test": "xo && nyc ava",
    "start": "claudia create --name aztable --region us-east-1 --api-module index",
    "update": "claudia update"
  },
  "author": "noogen <friends@niiknow.org>",
  "dependencies": {
    "claudia-api-builder": "^2.5.1"
  },
  "optionalDependencies": {
    "aws-sdk": "^2.96.0"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "claudia": "^2.14.2",
    "nyc": "^11.0.0",
    "sinon": "^4.2.2",
    "xo": "^0.18.2"
  },
  "engines": {
    "node": ">=4"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    ""
  ],
  "xo": {
    "semicolon": true,
    "prefer-const": false,
    "ignore": [
      "node_modules",
      "coverage",
      "db",
      "datasets",
      ".nyc_output"
    ],
    "space": 2,
    "rules": {
      "object-shorthand": [
        0,
        "consistent"
      ],
      "capitalized-comments": [
        "error",
        "never"
      ],
      "ava/prefer-async-await": 0
    }
  },
  "ava": {
    "failWithoutAssertions": true
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "coverage",
      "db",
      "datasets",
      ".nyc_output"
    ]
  }
}
